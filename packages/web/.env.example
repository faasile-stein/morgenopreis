# Supabase (get from: cd packages/database && npm run supabase:start)
NEXT_PUBLIC_SUPABASE_URL=http://127.0.0.1:54321
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key-from-supabase-start

# Custom API (for complex business logic)
NEXT_PUBLIC_API_URL=http://localhost:3001

# Drupal CMS (Stage 3 - SEO content)
NEXT_PUBLIC_DRUPAL_URL=http://localhost:8080

# Google Maps
NEXT_PUBLIC_GOOGLE_MAPS_KEY=

# Analytics
NEXT_PUBLIC_GA_TRACKING_ID=
NEXT_PUBLIC_ANALYTICS_ENABLED=false

# Feature Flags
NEXT_PUBLIC_ENABLE_WHEEL=true
NEXT_PUBLIC_ENABLE_PRICE_ALERTS=true

# Environment
NEXT_PUBLIC_ENV=development
